    "@typescript-eslint/eslint-plugin": "^6.11.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-n": "^15.4.0",
    "eslint": "^8.54.0",
    "esbuild": "^0.16.14",
    "@typescript-eslint/parser": "^6.11.0",
    "eslint-config-standard-with-typescript": "^40.0.0",
    "prettier": "^3.1.0",
    "ts-node": "^10.8.0",
    "typescript": "^5.2.2",
    "jest": "^29.7.0",
